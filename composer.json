{
    "name": "lankit/datatables-bundle",
    "description": "DataTables.js server-side processing for Doctrine Entities",
    "keywords": ["LanKitDatatables","LanKit","DataTables"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chad Sikorra",
            "email": "Chad.Sikorra@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.0 || ~3.0.0",
        "doctrine/orm": "~2.0",
        "doctrine/doctrine-bundle": "~1.0"
    },
    "suggest": {
        "jms/serializer-bundle": "Advanced JSON encoding capabilities, recommended."
    },
    "autoload": {
        "psr-0": { "LanKit\\DatatablesBundle": "" }
    },
    "target-dir": "LanKit/DatatablesBundle"
}
